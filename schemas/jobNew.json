{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "salary",
        "equity",
        "company_handle"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Valet Attendant"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                100000
            ]
        },
        "equity": {
            "type": "numeric",
            "default": 0.0,
            "title": "The equity Schema",
            "examples": [
                0.4
            ]
        },
        "company_handle": {
            "type": "string",
            "default": "",
            "title": "The company_handle Schema",
            "examples": [
                "ABM"
            ]
        }
    },
    "examples": [{
        "title": "Valet Attendant",
        "salary": 100000,
        "equity": 0.4,
        "company_handle": "ABM"
    }]
}